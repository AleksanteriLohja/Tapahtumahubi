{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET MAUI (Windows)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-windows",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/src/Tapahtumahubi.App/Tapahtumahubi.App.csproj",
        "-f",
        "net8.0-windows10.0.19041.0"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "env": {
        "DOTNET_WATCH": "0"
      }
    }
  ]
}